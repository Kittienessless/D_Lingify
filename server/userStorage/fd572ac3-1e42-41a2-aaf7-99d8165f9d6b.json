      }
    ],
    "pronoun": [
      {
        "key": "1",
        "rule": "Местоимения имеют формы единственного и множественного числа."
      },
      {
        "key": "2",
        "rule": "Форма местоимения зависит от рода подлежащего."
      }
    ],
    "adjective": [
      {
        "key": "1",
        "rule": "Прилагательные согласуются с определяемым существительным в роде и числе."
      },
      {
        "key": "2",
        "rule": "Степени сравнения образуются добавлением префиксов 'en-' и 'nelya'."
      }
    ],
    "adverb": [
      {
        "key": "1",
        "rule": "Наречия обычно следуют за прилагательными и образуют степени сравнения аналогично прилагательным."
      }
    ],
    "conjunction": [
      {
        "key": "1",
        "rule": "Союз 'ala' используется для выражения противопоставления."
      }
    ]
